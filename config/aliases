alias tkd="/Users/gkrohn/code/treekanga_work/dev/treekanga"
function tkdev {
  local folder
  folder=$(find ~/code/treekanga_work -mindepth 1 -maxdepth 1 -type d | fzf --prompt="Select a worktree: ")
  if [[ -n "$folder" ]]; then
    # echo "$folder" # for debugging
    "$folder"/treekanga "$@" || return
  fi
}

